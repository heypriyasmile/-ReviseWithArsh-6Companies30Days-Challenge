class Solution {
    public int maxRotateFunction(int[] nums) {
        int sum = 0;
        int n = nums.length;
        for(int i = 0;i < n;i++)
            sum += nums[i];
        int maxi = -10000;
        int a = 0;
        for(int i = 0;i < n;i++)
            a += nums[i]*i;
        maxi = a;
        for(int j = n - 1; j >= 1;j--){
            a = a + sum - n*nums[j];
            maxi = Math.max(a,maxi);
        }
        return maxi;
    }
}
